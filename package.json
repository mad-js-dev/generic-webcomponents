{
  "name": "@mad-js-dev/generic-webcomponents",
  "version": "0.2.3",
  "description": "A collection of reusable web components that work across different frameworks",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mad-js-dev/generic-webcomponents.git"
  },
  "homepage": "https://mad-js-dev.github.io/generic-webcomponents",
  "keywords": [
    "webcomponents",
    "react",
    "vue",
    "ui",
    "components",
    "custom-elements",
    "collapsible"
  ],
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.css",
    "dist/**/*.map"
  ],
  "types": "./dist/types/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "default": "./dist/index.es.js"
    },
    "./components": {
      "types": "./dist/types/components/index.d.ts",
      "import": "./dist/components/index.js",
      "default": "./dist/components/index.js"
    },
    "./components/*": {
      "types": "./dist/types/components/*/index.d.ts",
      "import": "./dist/components/*.js",
      "default": "./dist/components/*.js"
    }
  },
  "browser": "./dist/index.es.js",
  "scripts": {
    "dev": "vite",
    "build": "npm run clean && npm run build:components",
    "build:components": "vite build --config vite.components.config.js",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build",
    "prepare": "husky install",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:coverage": "jest --coverage",
    "predeploy": "npm run build:docs",
    "deploy": "node gh-pages.js"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ]
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "vue": ">=3.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "vue": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/plugin-transform-runtime": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.6.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "babel-jest": "^30.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^6.3.0",
    "husky": "^9.1.7",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "terser": "^5.43.1",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^3.7.1"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "rimraf": "^6.0.1"
  }
}
